{
  "name": "void/void",
  "description": "void",
  "license": "MIT",
  "require": {
    "php": "^7.4",
    "nikolaposa/version": "^3.2",
    "wyrihaximus/constants": "^1.5"
  },
  "require-dev": {
    "wyrihaximus/test-utilities": "^2.0"
  },
  "config": {
    "platform": {
      "php": "7.4"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "WyriHaximus\\Github\\Actions\\NextSemVers\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "WyriHaximus\\Tests\\Github\\Actions\\NextSemVers\\": "tests/"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "composer normalize || true"
    ],
    "post-update-cmd": [
      "composer normalize || true"
    ]
  }
}
